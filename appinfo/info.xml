<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>cas</id>
    <name>CAS Server</name>
    <summary>CAS protocol server (IdP)</summary>
    <description><![CDATA[This server implements a basic version of the CAS single sign-on protocol.]]></description>
    <version>0.2.1</version>
    <licence>agpl</licence>
    <author>Marco Ziech &lt;marco+nc@ziech.net&gt;</author>
    <namespace>Cas</namespace>
    <types>
        <authentication/>
    </types>
    <category>integration</category>
    <website>https://github.com/mziech/nextcloud-cas</website>
    <discussion>https://github.com/mziech/nextcloud-cas/discussions</discussion>
    <bugs>https://github.com/mziech/nextcloud-cas/issues</bugs>
    <repository>https://github.com/mziech/nextcloud-cas</repository>
    <screenshot>https://raw.githubusercontent.com/mziech/nextcloud-cas/master/img/screenshot.png</screenshot>
    <dependencies>
        <owncloud min-version="10"/>
        <nextcloud min-version="15" max-version="22"/>
    </dependencies>
    <background-jobs>
        <job>OCA\Cas\Job\TicketCleanupJob</job>
    </background-jobs>
    <settings>
        <admin>OCA\Cas\Settings\Admin</admin>
        <admin-section>security</admin-section>
    </settings>
</info>
